<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <font
        android:font="@font/playfair_display_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/playfair_display_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <font
        android:font="@font/playfair_display_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/playfair_display_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <font
        android:font="@font/playfair_display_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/playfair_display_medium"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <font
        android:font="@font/playfair_display_medium_italic"
        android:fontStyle="italic"
        android:fontWeight="500"
        app:font="@font/playfair_display_medium_italic"
        app:fontStyle="italic"
        app:fontWeight="500" />

    <font
        android:font="@font/playfair_display_semi_bold"
        android:fontStyle="normal"
        android:fontWeight="600"
        app:font="@font/playfair_display_semi_bold"
        app:fontStyle="normal"
        app:fontWeight="600" />

    <font
        android:font="@font/playfair_display_semi_bold_italic"
        android:fontStyle="italic"
        android:fontWeight="600"
        app:font="@font/playfair_display_semi_bold_italic"
        app:fontStyle="italic"
        app:fontWeight="600" />

    <font
        android:font="@font/playfair_display_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/playfair_display_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <font
        android:font="@font/playfair_display_bold_italic"
        android:fontStyle="italic"
        android:fontWeight="700"
        app:font="@font/playfair_display_bold_italic"
        app:fontStyle="italic"
        app:fontWeight="700" />

    <font
        android:font="@font/playfair_display_extra_bold"
        android:fontStyle="normal"
        android:fontWeight="800"
        app:font="@font/playfair_display_extra_bold"
        app:fontStyle="normal"
        app:fontWeight="800" />

    <font
        android:font="@font/playfair_display_extra_bold_italic"
        android:fontStyle="italic"
        android:fontWeight="800"
        app:font="@font/playfair_display_extra_bold_italic"
        app:fontStyle="italic"
        app:fontWeight="800" />

    <font
        android:font="@font/playfair_display_black"
        android:fontStyle="normal"
        android:fontWeight="900"
        app:font="@font/playfair_display_black"
        app:fontStyle="normal"
        app:fontWeight="900" />

    <font
        android:font="@font/playfair_display_black_italic"
        android:fontStyle="italic"
        android:fontWeight="900"
        app:font="@font/playfair_display_black_italic"
        app:fontStyle="italic"
        app:fontWeight="900" />

</font-family>